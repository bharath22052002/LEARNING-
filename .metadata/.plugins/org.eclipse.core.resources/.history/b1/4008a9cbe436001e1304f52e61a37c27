package conditionif;

public class NestedSimpleif {
    public static void main(String[] args) {
        int x = 10;
        int y = 5;
        if(x>0)
        {
        	System.out.println("it is possible");
            if(y>0)
            {
        	  System.out.println("y is possible");
            }
        }
    }
}
        



